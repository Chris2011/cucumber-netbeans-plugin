<?xml version="1.0" encoding="UTF-8"?>
<!-- Author : Roland Guem -->

<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-feature">
                <folder name="CodeTemplates">
                    <file name="snippets.xml" url="snippets.xml"/>
                </folder>
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
                <folder name="FoldManager">
                    <file name="org-netbeans-modules-languages-features-LanguagesFoldManager$Factory.instance"/>
                </folder>
                <folder name="SideBar">
                    <file name="org-netbeans-modules-languages-features-CodeFoldingSideBarFactory.instance">
                        <attr name="position" intvalue="1500"/>
                    </file>
                </folder>
                <folder name="UpToDateStatusProvider">
                    <file name="org-netbeans-modules-languages-features-UpToDateStatusProviderFactoryImpl.instance"/>
                </folder>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <file name="language.nbs" url="feature.nbs">
                    <attr name="icon" stringvalue="info/cukes/feature/resources/cucumber.png"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-feature" url="templates/sample.feature"/>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="info_cukes_feature_update_center.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url_key" stringvalue="info_cukes_feature_update_center"/>
            </file>
        </folder>
        <folder name="MIMEResolver">
            <file name="FeatureResolver.xml" url="FeatureResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="position" intvalue="100000"/>
            </file>
        </folder>
    </folder>
    <folder name="Navigator">
        <folder name="Panels">
            <folder name="text">
                <folder name="x-feature">
                    <file name="org-netbeans-modules-languages-features-LanguagesNavigator.instance"/>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Templates">
        <folder name="Cucumber">
            <file name="file.feature" url="templates/file.feature">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="template" boolvalue="true"/>
            </file>
            <file name="FeatureTemplate.feature" url="templates/FeatureTemplate.feature">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="template" boolvalue="true"/>
            </file>
            <file name="german.feature" url="templates/german.feature">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="template" boolvalue="true"/>
            </file>
            <file name="french.feature" url="templates/french.feature">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="template" boolvalue="true"/>
            </file>
            <file name="portuguese.feature" url="templates/portuguese.feature">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="template" boolvalue="true"/>
            </file>
            <file name="spanish.feature" url="templates/spanish.feature">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="template" boolvalue="true"/>
            </file>
        </folder>
    </folder>
</filesystem>