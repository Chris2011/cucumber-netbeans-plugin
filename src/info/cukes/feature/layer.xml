                    </file>
                </folder>
                <folder name="UpToDateStatusProvider">
                    <file name="org-netbeans-modules-languages-features-UpToDateStatusProviderFactoryImpl.instance"/>
                </folder>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <file name="language.nbs" url="feature.nbs">
                    <attr name="icon" stringvalue="info/cukes/feature/resources/cucumber.png"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-feature" url="templates/sample.feature"/>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="info_cukes_feature_update_center.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url_key" stringvalue="info_cukes_feature_update_center"/>
            </file>
        </folder>
        <folder name="MIMEResolver">
            <file name="FeatureResolver.xml" url="FeatureResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="info.cukes.feature.Bundle"/>
                <attr name="position" intvalue="100000"/>
            </file>
        </folder>
    </folder>
